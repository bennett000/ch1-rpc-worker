{
  "name": "@ch1/rpc-worker",
  "description": "JavaScript Remote Procedure Call (RPC) - Web Worker Adapter",
  "keywords": [
    "rpc",
    "remote",
    "procedure",
    "call",
    "parallel",
    "worker",
    "socket",
    "socket.io",
    "webworker",
    "postMessage"
  ],
  "version": "1.1.0",
  "author": {
    "name": "Michael J. Bennett",
    "email": "dev@michaeljbennett.info",
    "url": "https://michaeljbennett.info"
  },
  "contributors": [],
  "license": "LGPL-3.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/bennett000/ch1-rpc-worker"
  },
  "homepage": "https://github.com/bennett000/ch1-rpc-worker",
  "bugs": {
    "url": "https://github.com/bennett000/ch1-rpc-worker/issues"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "tsc",
    "postbuild": "rollup --input dist/index.js --format cjs --sourcemap --file dist/index.cjs.js",
    "clean": "rimraf ./intermediate && rimraf ./dist",
    "precommit": "lint-staged",
    "pretest": "rimraf coverage && yarn build && rollup -c",
    "test": "karma start ./karma.conf.js"
  },
  "devDependencies": {
    "@types/node": "^10.3.5",
    "husky": "^0.14.3",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.5",
    "rimraf": "^2.5.2",
    "rollup": "^0.65.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "typescript": "^3.0.3",
    "yarn": "^1.9.4"
  },
  "lint-staged": {
    "*.{ts,js,json,css,md}": [
      "prettier --write --single-quote --trailing-comma all --print-width 78",
      "git add"
    ]
  },
  "dependencies": {
    "@ch1/rpc": "^1.3.1",
    "@ch1/utility": "^0.5.1"
  }
}
